{
  "name": "@rs-tech-hub/nestjs-demo-backend",
  "version": "1.0.0",
  "description": "A demo backend for rs-tech-hub modules",
  "main": "index.js",
  "author": "RS Tech Hub",
  "license": "MIT",
  "scripts": {
    "prisma:copy:client": "node scripts/copy-prisma-client.js",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "prisma:gen": "npx prisma generate --schema prisma/starter-schema.prisma && yarn prisma:copy:client",
    "prisma:migrate:dev": "npx prisma migrate dev --schema prisma/starter-schema.prisma",
    "prisma:dev:up": "yarn prisma:gen && yarn prisma:migrate:dev",
    "docker:start:db": "docker start nestjs-demo-backend-db",
    "docker:init:db": "yarn docker:start:db && yarn prisma:dev:up && yarn prisma:copy:client",
    "docker:build:dev": "docker-compose --project-name rs-tech-hub-nestjs-demo-backend -f docker-compose.dev.yaml up",
    "test:api": "node scripts/testing/test-runner.js"
  },
  "dependencies": {
    "@apollo/server": "^5.2.0",
    "@as-integrations/express5": "^1.1.2",
    "@nestjs/apollo": "^13.1.0",
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.9",
    "@nestjs/graphql": "^13.2.3",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cookie-parser": "^1.4.7",
    "express-basic-auth": "^1.2.1",
    "express-session": "^1.18.2",
    "graphql": "^16.12.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.17.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "express": "^4.21.2",
    "node-gyp-build": "^4.8.4",
    "rimraf": "^6.0.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.9",
    "@types/express-session": "^1.18.2",
    "prisma": "^7.3.0",
    "tsconfig-paths": "^4.2.0"
  }
}
