{
  "name": "@rs-tech-hub/nestjs-demo-backend",
  "version": "1.0.0",
  "description": "demo backend for rs-tech-hub modules",
  "main": "index.js",
  "author": "RS Tech Hub",
  "license": "MIT",
  "scripts": {
    "postinstall": "node scripts/copy-prisma-client.js",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "prisma:gen": "npx prisma generate --schema prisma/starter-schema.prisma",
    "prisma:migrate:dev": "npx prisma migrate dev --schema prisma/starter-schema.prisma",
    "prisma:dev:up": "yarn prisma:gen && yarn prisma:migrate:dev",
    "docker:start:db": "docker start nestjs-demo-backend-db",
    "docker:start:dev": "yarn docker:start:db && yarn prisma:dev:up",
    "docker:build:dev": "docker-compose --project-name rs-tech-hub-nestjs-demo-backend -f docker-compose.dev.yaml up"
  },
  "dependencies": {
    "@apollo/server": "^5.2.0",
    "@as-integrations/express5": "^1.1.2",
    "@nestjs/apollo": "^13.1.0",
    "@nestjs/common": "^11.1.9",
    "@nestjs/core": "^11.1.9",
    "@nestjs/graphql": "^13.2.3",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/throttler": "^6.4.0",
    "@rs-tech-hub/nestjs-account-starter": "^1.0.1",
    "@rs-tech-hub/nestjs-auth-core": "^1.0.0",
    "@rs-tech-hub/nestjs-auth-starter": "^1.0.3",
    "@rs-tech-hub/nestjs-clock": "^1.0.0",
    "@rs-tech-hub/nestjs-common-interceptors": "^1.0.1",
    "@rs-tech-hub/nestjs-prisma": "^1.0.0",
    "@rs-tech-hub/nestjs-profile": "^1.0.1",
    "@rs-tech-hub/nestjs-service-operation": "^1.0.0",
    "@rs-tech-hub/nestjs-test-starter": "^1.0.2",
    "@rs-tech-hub/nestjs-user": "^1.0.1",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "express-session": "^1.18.2",
    "graphql": "^16.12.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.9",
    "@types/express-session": "^1.18.2",
    "prisma": "^7.2.0"
  }
}
